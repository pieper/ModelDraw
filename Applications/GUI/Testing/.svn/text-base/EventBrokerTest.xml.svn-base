<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Testing</category>
  <title>Event Broker Test</title>
  <description>
Test the Event Broker on some mrml and gui elements.
  </description>
  <version>3.0</version>
  <documentation-url>http://www.slicer.org</documentation-url>
  <license>slicer3</license>
  <contributor>Steve Pieper</contributor>
<acknowledgements>
This work is part of the National Alliance for Medical Image Computing (NAMIC), funded by the National Institutes of Health through the NIH Roadmap for Medical Research, Grant U54 EB005149.
</acknowledgements>

<parameters>

    <file>
      <name>logFileName</name>
      <longflag>logFileName</longflag>
      <description>If specified, file to log all event information.</description>
      <label>Log File Name</label>
      <channel>output</channel>
    </file>

    <file>
      <name>graphFile</name>
      <longflag>graphFile</longflag>
      <description>If specified, generate a file in dot (Graphviz) format showing all the observations registered in the broker.</description>
      <label>Graph File</label>
      <channel>output</channel>
    </file>

</parameters>

</executable>
